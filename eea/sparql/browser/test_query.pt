<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en-US"
  lang="en-US"
  metal:use-macro="here/main_template/macros/master"
  i18n:domain="eea">

<body>

<div metal:fill-slot="content-core" tal:define="options view/test_results">
<table class="sparql-results" tal:condition="options">
  <thead>
    <tr>
      <th tal:repeat="name options/var_names"
          tal:content="name"/>
    </tr>
  </thead>
  <tbody>
    <tr tal:repeat="row options/rdfterm_rows">
      <td tal:repeat="value row"
          tal:content="value/n3|string:&quot;${value}&quot;"/>
    </tr>
  </tbody>
</table>

</div>
</body>
</html>
